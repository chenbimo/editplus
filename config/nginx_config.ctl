#T=rewrite
rewrite regex replacement [flag];^!
#T=server {
server {
    ^!
}
#T=listen 80;
#T=listen 443;
#T=server_name
server_name domain.com^!;
#T=root
root /home/wwwroot/^!;
#T=index
index index.html index.htm index.php^!;
#T=alias
alias ^!;
#T=access_log
access_log  /home/wwwlogs/site.log^!  access;
