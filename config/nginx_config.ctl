#T=== Frequently used ==
#T=if ( ) {
if (...) {
    ^!;
}
#T=if ( ) { rewrite
if (...) {
	rewrite ^!;
}
#T=set $var
set $var ^!;
#T=location = / {
location = / {
    ^!
}
#T=location alias
location = /vdir/ {
    alias /realdir/^!;
}
#T=$https
